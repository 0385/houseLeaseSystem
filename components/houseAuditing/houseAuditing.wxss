.goods {
  display: inline-block;
  width: 160rpx;
  height: 32rpx;
  background-color: #eee;
  border-radius: 6rpx;
  font-size: 26rpx;
  line-height: 32rpx;
  text-align: center;
  color: #aaa;
}

.goods1 {
  position: absolute;
  left: -21rpx;
  top: 120rpx;
}

.goods2 {
  position: absolute;
  left: 180rpx;
  top: 120rpx;
}
.auditing{
  display: inline-block;
  width: 150rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
  background-color: #fff;
  z-index: 999;
  position: absolute;
  left: 60rpx;
  top: 0rpx;
  opacity: 0.6;

}