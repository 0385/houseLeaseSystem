.tui-gallery-list {
  display: -webkit-flex;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
  position: relative;
}

.gallery-img {
  width: 271rpx;
  height: 245rpx;
  margin-left: 58rpx;
  flex: 0 0 auto;
  border-radius: 10rpx;
}

.gallery-box {
  margin-top: 0rpx;
  width: 400rpx;
  height: 185rpx;
  overflow: hidden;
  box-sizing: border-box;
}

.gallery-name {
  display: block;
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  font-weight: bold;
  position: absolute;
  font-size: 35rpx;
  top: -12rpx;
  color: rgb(5, 5, 5);
}
.gallery-lease_term{
  right: 27rpx;
  top:85rpx;
    color: #8f8f94;
}
.gallery-start_time{
 right: 29rpx;
  top:125rpx;
  color: #8f8f94;
}
.gallery-end_time{
 right: 29rpx;
  top:165rpx;
  color: #8f8f94;
}
.gallery-comon {
  display: block;
  width: 400rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-family: '楷体';
  position: absolute;
}

.gallery-detail {
  overflow: hidden;
  font-size: 27rpx;
  color: #8f8f94;
  white-space: nowrap;
  text-overflow: ellipsis;
  top: 30rpx;
}



.gallery-location {
  top: 75rpx;
  color: #8f8f94;
}

.gallery-money {
  top: 30rpx;
  right: -140rpx;
  color: rgb(250, 200, 30);
  font-size: 32rpx;
}

 .gallery-location, .gallery-detail, .gallery-name {
  right: 5rpx;
}

.gallery-left {
  padding-left: 10px;
}

.gallery-right {
  padding-right: 10px;
}

