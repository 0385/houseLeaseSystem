.goods {
  display: inline-block;
  width: 160rpx;
  height: 32rpx;
  background-color: #eee;
  border-radius: 6rpx;
  font-size: 26rpx;
  line-height: 32rpx;
  text-align: center;
  color: #aaa;
}

.goods1 {
  position: absolute;
  left: -21rpx;
  top: 120rpx;
}

.goods2 {
  position: absolute;
  left: 180rpx;
  top: 120rpx;
}
.payBtn{
  width:100rpx;
  height: 55rpx;
  background-color: rgba(98, 37, 240,0.5);

  border-radius: 35rpx;
  text-align: center;
  line-height: 55rpx;
  position: absolute;
  right: 14rpx;
  top: 239rpx;
}
.gallery-money{
  flex: 4
}
.total_money{
  flex: 6;
}
