.goods {
  display: inline-block;
  width: 160rpx;
  height: 32rpx;
  background-color: #eee;
  border-radius: 6rpx;
  font-size: 26rpx;
  line-height: 32rpx;
  text-align: center;
  color: #aaa;
}

.goods1 {
  position: absolute;
  left: -21rpx;
  top: 120rpx;
}

.goods2 {
  position: absolute;
  left: 180rpx;
  top: 120rpx;
}
.payBtn{
  width:100rpx;
  height: 55rpx;
  background-color: rgba(98, 37, 240,0.5);
  z-index: 900;
  border-radius: 35rpx;
  text-align: center;
  line-height: 55rpx;
  position: absolute;
  right: 14rpx;
  top: 239rpx;
}
.maskPanel{
  position: absolute;
top: 0;
  width: 100%;
  height: 1210rpx;
background: rgba(0, 0, 0,0.3);
z-index: 902;
}
.payPanel{
  width: 100%;
  height: 500rpx;
  background-color:#fff;
  position: absolute;
  bottom: 0;
  z-index: 999;
}
.show{
  display: block
}
.hidden{
  display: none;
}
.pay_way{
  padding-top: 10rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #eee;
  display: inline-block;
  width: 100%;
  font-size: 40rpx;
  text-align: center;
}
.item_common{
  display: flex;
  justify-content: space-between;
  padding:10rpx 20rpx;

}
.item{
