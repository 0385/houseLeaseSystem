/* 底部弹出timeModel按钮 */



/* 是否显示 */
.modal_show {
    display: block;
}
.modal_hide {
    display: none;
}

/* 日期弹出框 */
.modal {
    position: fixed;
    z-index: 99;
    bottom: 0;
    width: 100%;
    height:auto;
    /* background: rgba(0, 0, 0, 0.3); */
}

/* 背景模板*/
.modal_cancel {
    position: absolute;
    bottom:447rpx;
    left: 0rpx;
    width: 80rpx;
    height: 80rpx;
    font-size: 69rpx;
    z-index: 100;
}
/* 弹出框主体 */
.modal_container {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: auto;
    background: #eee;
}

/* modal_header */
.modal_header {
    padding: 20rpx;
    text-align: center;
    font-size: 11pt;
}
/* header_tip */
.header_tip {
    position: relative;
    font-size: 9pt;
    color: #999;
}

/* time日期主体view */
.time {
    background: #f7f7f7;
    text-align: center;
    font-size: 11pt;
}
/* time日期主体滚动view */
.time_scroll {
    white-space: nowrap;
    width: 100%;
    height:100rpx;
    line-height:50rpx;
}

.scroll-view-item {
    height: 100rpx;
}
/* 日期 */
.scroll_item {
    display: inline-block;
    padding: 0 40rpx;
    height: 100rpx;
    box-sizing: border-box;
   background-color: #eee;
}

/* time_active选择状态 */
.time_active {
    color: #159de3;
    border-bottom: 4rpx solid #159de3;
}

/* 时间 */
.hour {
    padding: 20rpx 0 0 20rpx;
    position: relative;
}

.hour_item {
    width: 22%;
    box-sizing: border-box;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 10rpx;
    text-align: center;
    float: left;
    border: 2rpx solid #eee;
    font-size: 11pt;
    margin-right: 3%;
    margin-bottom: 20rpx;
}

/* hour_active*/

.hour_active {
    background: #159de3;
    color: #fff;
    border: 2rpx solid #159de3;
}

/* hour_unactive */

.hour_unactive {
    color: #999;
}
